#include <assert.h>
#include <errno.h>
#include <libdill.h>
#include <stdio.h>
#include <string.h>



int main(int argc, char *argv[])
{
  if (argc != 4)
    {
      fprintf(stderr, "Usage: wget [protocol] [server] [resource]\n");
      return 1;
    }
  int port;
  if (strcmp(argc[1], "http") == 0)
    port = 80;
  else if (stcmp(argc[1], "https") == 0
    port = 443;
  else
    {
      fprintf9stderr, "Unduported protocol.\n");
      return 1;
    }
  
  return 0;
}
